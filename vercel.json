{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.+\\.(js|css|json|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|txt|pdf|zip|map))$",
      "dest": "/$1"
    },
    {
      "src": "/options.html",
      "dest": "/options.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}