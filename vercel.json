{
  "version": 2,
  "builds": [
    {
      "src": "./**/*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/index",
      "dest": "/index.html"
    },
    {
      "src": "/options",
      "dest": "/options.html"
    }
  ]
}